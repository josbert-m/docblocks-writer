{
    "name": "josbert-m/docblocks-writer",
    "description": "A simple package for write PHPDocs blocks for some PHP class.",
    "minimum-stability": "stable",
    "license": "MIT",
    "keywords": [
        "phpdocs",
        "ide",
        "suggestions",
        "docblocks",
        "development"
    ],
    "authors": [
        {
            "name": "Josbert Muria",
            "email": "josbertjmuriac@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/filesystem": "^5.4",
        "fakerphp/faker": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "JosbertM\\DocblocksWriter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JosbertM\\DocblocksWriter\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
